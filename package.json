{
  "name": "ngx-chono-ui-kit",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:core": "ng build core --configuration=ivy",
    "postbuild:core": "npx ts-node postbuild-core && npm run sass:core && npm run postcss:core",
    "prepublish:core": "ng build core --configuration=ivy && npx ts-node postbuild-core && npm run sass:core && npm run postcss:core && ng test --browsers ChromeHeadless",
    "watch:core": "concurrently \"npm:sass:core -- --watch\" \"npm run build:core -- --watch\" \"npm run postcss:core --watch\"",
    "sass:core": "sass ./projects/core/scss/_index.scss ./projects/core/scss/_index.css",
    "postcss:core": "postcss build ./projects/core/scss/_index.css -o ./dist/core/scss/_index.css --env production --config ./projects/core/ && npx ts-node ./projects/core/postcss-components",
    "lint": "eslint --ignore-path ./projects/eslint-config/.eslintignore --ext .html,.js,.ts,.jsx,.tsx projects/core/** --fix"
  },
  "dependencies": {
    "@angular/animations": "^12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@schematics/angular": "^12.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "~12.2.0",
    "@angular/platform-browser-dynamic": "^12.2.0",
    "@ngx-chrono-ui-kit/eslint-config": "file:projects/eslint-config",
    "@ngx-chrono-ui-kit/stylelint-config": "file:projects/stylelint-config",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^17.0.31",
    "@types/node-fetch": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cssnano": "^5.0.16",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.19",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^12.1.1",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "postcss-merge-rules": "^5.0.6",
    "postcss-scss": "^4.0.3",
    "prettier": "2.5.1",
    "typescript": "~4.3.5"
  }
}
